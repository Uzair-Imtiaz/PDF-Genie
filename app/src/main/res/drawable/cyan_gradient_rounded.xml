<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="#00FFFF" /> <!-- Cyan color -->
            <corners android:radius="7dp" /> <!-- Adjust the radius as needed for rounded corners -->
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <gradient
                android:startColor="#93B1A6"
                android:centerColor="#5C8374"
                android:endColor="#183D3D"
            android:type="linear"
            android:angle="45"/>
            <corners android:radius="8dp" />
        </shape>
    </item>
</layer-list>
